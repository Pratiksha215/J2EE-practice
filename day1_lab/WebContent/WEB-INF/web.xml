<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>day1_lab</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<!-- servlet deployment tags -->
	<servlet>
		<servlet-name>abc</servlet-name><!-- key in map -->
		<servlet-class>pages.SecondServlet</servlet-class><!-- Fully qualified class name i.e value in map -->
	</servlet>
	<servlet-mapping>
		<servlet-name>abc</servlet-name>
		<url-pattern>/test2</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>abc1</servlet-name><!-- key in map -->
		<servlet-class>pages.InputServlet</servlet-class><!-- Fully qualified class name i.e value in map -->
	</servlet>
	<servlet-mapping>
		<servlet-name>abc1</servlet-name>
		<url-pattern>/test_input</url-pattern>
	</servlet-mapping>
</web-app>